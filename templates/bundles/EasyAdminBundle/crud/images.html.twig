<div class="form-widget form-widget-custom">
    <div class="_data-row data-row-custom field-image">
    {% for image in field.value %}
        <div class="col">
            <a href="#" class="ea-lightbox-thumbnail" data-ea-lightbox-content-selector="#ea-lightbox-{{ image.id }}">
                <img src="/uploads/items/{{ image.image }}" class="img-fluid" width="170" alt="">
            </a>
            <div id="ea-lightbox-{{ image.id }}" class="ea-lightbox">
                <img src="/uploads/items/{{ image.image }}" width="170" alt="">
            </div>
        </div>
    {% endfor %}
    </div>
</div>